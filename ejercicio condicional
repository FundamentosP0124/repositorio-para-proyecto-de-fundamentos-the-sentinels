#include<iostream>
using namespace std;
int main () {
char letra;
cout<< "digite un caractér: ";
cin>>letra;
switch (letra){
    case 'a':
    case 'e':
    case 'i':
    case 'o':
    case 'u': cout<<"es una vocal minúscula" ;break;
    default : cout<<"no es una vocal minúscula" ;break;
}
  return 0;
}